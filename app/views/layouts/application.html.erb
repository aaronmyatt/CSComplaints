<!DOCTYPE html>
<html>
<head>
  <title>CSComplaints</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>

  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>

  <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,600,700' rel='stylesheet' type='text/css'>
</head>
<body>


<%= yield %>


<%= render partial: 'shared/register_modal', locals: {id:'register-modal'} %>
<%= render partial: 'shared/login_modal', locals: {id:'login-modal'} %>
<% if current_user %>
		<%= render partial: 'shared/profile_modal', locals: {id:'profile-modal'} %>
<% end %>
</body>
</html>
